<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/colorBackground"
    android:layout_height="match_parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/qubitimage"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/logo_corner"
            android:clickable="true"
            android:padding="1dp"
            android:src="@drawable/qubit" />

        <ImageView
            android:layout_alignParentLeft="true"
            android:id="@+id/imageView2"
            android:layout_width="150dp"
            android:layout_height="130dp"
            android:layout_below="@id/qubitimage"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/logo_corner"
            android:clickable="true"
            android:padding="1dp"
            android:src="@drawable/vsplash" />

        <ImageView
            android:layout_below="@id/qubitimage"
            android:layout_alignParentRight="true"
            android:id="@+id/imageView3"
            android:layout_width="150dp"
            android:layout_height="130dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/logo_corner"
            android:clickable="true"
            android:padding="1dp"
            android:src="@drawable/diseno2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.129"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/qubitimage"
            app:layout_constraintVertical_bias="0.0" />

        <ImageView
            android:layout_alignParentLeft="true"
            android:layout_below="@id/imageView3"
            android:id="@+id/imageView4"
            android:layout_width="150dp"
            android:layout_height="130dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/logo_corner"
            android:clickable="true"
            android:padding="1dp"
            android:src="@drawable/connectivitiee2" />

        <ImageView
            android:layout_below="@id/imageView3"
            android:layout_alignParentRight="true"
            android:id="@+id/imageView6"
            android:layout_width="150dp"
            android:layout_height="130dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/logo_corner"
            android:clickable="true"
            android:padding="1dp"
            android:src="@drawable/glitz2" />

        <ImageView
            android:layout_alignParentLeft="true"
            android:layout_below="@id/imageView4"
            android:id="@+id/imageView5"
            android:layout_width="150dp"
            android:layout_height="130dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/logo_corner"
            android:clickable="true"
            android:padding="1dp"
            android:src="@drawable/taikuun" />

        <ImageView
            android:layout_alignParentRight="true"
            android:layout_below="@id/imageView4"
            android:id="@+id/imageView7"
            android:layout_width="150dp"
            android:layout_height="130dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/logo_corner"
            android:clickable="true"
            android:padding="1dp"
            android:src="@drawable/vitness" />

    </RelativeLayout>

</androidx.constraintlayout.widget.ConstraintLayout>